{
  "manifest_version": 3,
  "name": "Domain Catcher",
  "version": "1.0",
  "description": "Catches good domain names from expireddomains.net based on specific criteria",
"permissions": [
  "storage",
  "scripting",
  "tabs",
  "webRequest",
  "webRequestBlocking"
],
  "host_permissions": ["*://*.expireddomains.net/*"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*.expireddomains.net/*"],
      "js": ["content.js"]
    }
  ]
}
